<center><h2><%= "Welcome to the #{@nation}" %></h2></center>
<h3><%= "Nation Members: #{@members.count}" %>
<div id="members">
  <% @members[0..24].each do |member| %>
    <div id="member-<%= member.name %>">
      <%= "Name: #{member.name}" %><br>
      <img src=<%= member.photo_url %>>
      <%= "Allies: #{member.allies}" %><br>
      <%= "Enemies: #{member.enemies}" %><br>
      <%= "Affiliations: #{member.affiliation}" %><br>
    </div>
  <% end %>
</div>